{
  "title": "#Right command + fn to a modifier for function layer",
  "rules": [
    {
      "description": "Change right_command + fn + asdfgh... to function keys",
      "manipulators": [

        {
          "type": "basic",
          "from": {
            "key_code": "a",
            "modifiers": {"mandatory": ["fn", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "f1"}]
        }, {
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": {"mandatory": ["fn", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "f2"}]
        }, {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": {"mandatory": ["fn", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "f3"}]
        }, {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": {"mandatory": ["fn", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "f4"}]
        }, {
          "type": "basic",
          "from": {
            "key_code": "g",
            "modifiers": {"mandatory": ["fn", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "f5"}]
        }, {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {"mandatory": ["fn", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "f6"}]
        }, {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {"mandatory": ["fn", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "f7"}]
        }, {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {"mandatory": ["fn", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "f8"}]
        }, {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {"mandatory": ["fn", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "f9"}]
        }, {
          "type": "basic",
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {"mandatory": ["fn", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "f10"}]
        }, {
          "type": "basic",
          "from": {
            "key_code": "semicolon",
            "modifiers": {"mandatory": ["fn", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "f11"}]
        }, {
          "type": "basic",
          "from": {
            "key_code": "quote",
            "modifiers": {"mandatory": ["fn", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "f12"}]
        }

      ]
    }
  ]
}
