{
  "title": "#Right command + left shift to a modifier for symbol layer",
  "rules": [
    {
      "description": "Change right_command + left_shift + asdfgh... to symbol keys",
      "manipulators": [

        {
          "type": "basic",
          "from": {
            "key_code": "a",
            "modifiers": {"mandatory": ["left_shift", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "1", "modifiers": ["left_shift"]}]
        }, {
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": {"mandatory": ["left_shift", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "2", "modifiers": ["left_shift"]}]
        }, {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": {"mandatory": ["left_shift", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "3", "modifiers": ["left_shift"]}]
        }, {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": {"mandatory": ["left_shift", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "4", "modifiers": ["left_shift"]}]
        }, {
          "type": "basic",
          "from": {
            "key_code": "g",
            "modifiers": {"mandatory": ["left_shift", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "5", "modifiers": ["left_shift"]}]
        }, {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {"mandatory": ["left_shift", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "6", "modifiers": ["left_shift"]}]
        }, {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {"mandatory": ["left_shift", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "7", "modifiers": ["left_shift"]}]
        }, {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {"mandatory": ["left_shift", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "8", "modifiers": ["left_shift"]}]
        }, {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {"mandatory": ["left_shift", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "9", "modifiers": ["left_shift"]}]
        }, {
          "type": "basic",
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {"mandatory": ["left_shift", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "0", "modifiers": ["left_shift"]}]
        }, {
          "type": "basic",
          "from": {
            "key_code": "semicolon",
            "modifiers": {"mandatory": ["left_shift", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "hyphen", "modifiers": ["left_shift"]}]
        }, {
          "type": "basic",
          "from": {
            "key_code": "quote",
            "modifiers": {"mandatory": ["left_shift", "right_command"], "optional": ["any"]}
          },
          "to": [{"key_code": "equal_sign", "modifiers": ["left_shift"]}]
        }

      ]
    }
  ]
}
